# Database
DATABASE_URL="postgresql://user:password@localhost:5432/vcn_ops_dashboard?schema=public"

# API Authentication
INGEST_TOKEN="your_secure_random_token_here"  # Token for /api/ingest endpoint

# Vercel API
VERCEL_TOKEN="your_vercel_token_here"
VERCEL_TEAM_ID="your_team_id_here" # Optional, only if using team

# GitHub API
GITHUB_TOKEN="your_github_token_here"
GITHUB_ORG="your_github_org_name"  # Optional, for organization billing

# Twilio API (future)
# TWILIO_ACCOUNT_SID="your_twilio_account_sid_here"
# TWILIO_AUTH_TOKEN="your_twilio_auth_token_here"

# AWS API
AWS_ACCESS_KEY_ID="your_aws_access_key_here"
AWS_SECRET_ACCESS_KEY="your_aws_secret_key_here"
AWS_REGION="us-east-1"  # Optional, defaults to us-east-1

# Cloudflare API
CLOUDFLARE_API_TOKEN="your_cloudflare_token_here"
CLOUDFLARE_ZONE_ID="your_cloudflare_zone_id_here"
